<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.bank.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.bank.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AccountCreate</servlet-name>
    <servlet-class>com.bank.AccountCreateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SavingsAccount</servlet-name>
    <servlet-class>com.bank.SavingsAccountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FixedAccount</servlet-name>
    <servlet-class>com.bank.FixedAccountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoanAccount</servlet-name>
    <servlet-class>com.bank.LoanAccountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccountCreate</servlet-name>
    <url-pattern>/account</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SavingsAccount</servlet-name>
    <url-pattern>/savAccount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FixedAccount</servlet-name>
    <url-pattern>/fixAccount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoanAccount</servlet-name>
    <url-pattern>/loaAccount</url-pattern>
  </servlet-mapping>
<display-name>Archetype Created Web Application</display-name>
</web-app>